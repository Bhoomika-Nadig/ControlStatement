package com.xworkz.Crud;

public class AirportStarter {

	public static void main(String[] args) {
		AirportStarter airport=new AirportStarter();
		airport.addName("Banglore airport");
		airport.addName("mysore airport");
		airport.addName("singapur airport");
		airport.addName("goa airport");
		airport.addName("manglore airport");

	}

}
